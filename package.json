  "scripts": {
    "build:pro": "cross-env NODE_ENV=production SERVER_ENV=production BUILD_ENV=production webpack --config ./config/webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development SERVER_ENV=release BUILD_ENV=production webpack --config ./config/webpack.config.js",
    "build:test": "cross-env NODE_ENV=test SERVER_ENV=release BUILD_ENV=production webpack --config ./config/webpack.config.js",
    "build:release": "cross-env NODE_ENV=release SERVER_ENV=release BUILD_ENV=production webpack --config ./config/webpack.config.js",
    "start": "cross-env NODE_ENV=development SERVER_ENV=development BUILD_ENV=development webpack-dev-server --hot --config ./config/webpack.config.js",
    "commitlint": "commitlint -e $GIT_PARAMS",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "eslint": "eslint src --ext .ts",
    "precommit": "lint-staged",
    "build:components": "webpack --config ./config/webpack.conf.js && cross-env NODE_ENV=component webpack --config ./config/webpack.component.js",
    "pub": "",
    "test": "webpack --config ./config/webpack.test.js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "standard": "^17.0.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6"
